<MudNavMenu>

  @foreach (var (name, path) in Stuff)
  {
    <MudNavLink Href="@path">@name</MudNavLink>
  }

</MudNavMenu>

@code {

  public static IEnumerable<(string name, string path)> Stuff
  {
    get
    {
      yield return ("SCL Playground", "playground");
      yield return ("Generate Schema from JSON", "schemageneratefromjson");
      yield return ("Generate Schema from CSV", "schemageneratefromcsv");
      yield return ("Validate JSON", "schemavalidatejson");
      yield return ("Validate CSV", "schemavalidatecsv");
      yield return ("Convert CSV to JSON", "convertcsvtojson");
      yield return ("Convert JSON to CSV", "convertjsontocsv");
    }
  }

}
