    <Nullable>enable</Nullable>
    <AssemblyName>SCLEditor.Blazor</AssemblyName>
    <RootNamespace>Reductech.Utilities.SCLEditor.Blazor</RootNamespace>
    <nullable>enable</nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BlazorDownloadFile" Version="2.1.6" />
    <PackageReference Include="BlazorMonaco" Version="2.1.0" />
    <PackageReference Include="MELT" Version="0.8.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0" PrivateAssets="all" />
    <PackageReference Include="MudBlazor" Version="6.0.2" />
    <PackageReference Include="System.Net.Http.Json" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SCLEditor\SCLEditor.csproj" />
  </ItemGroup>

</Project>
